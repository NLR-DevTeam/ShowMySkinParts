{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "icon": "assets/${mod_id}/icon.png",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    {
      "name": "XIAYM-gh",
      "contact": {
        "homepage": "https://github.com/XIAYM-gh"
      }
    }
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "issues": "${mod_sources}/issues",
    "sources": "${mod_sources}"
  },
  "license": "${mod_license}",
  "environment": "client",
  "entrypoints": {
    "modmenu": [
      "cn.xiaym.skin.compat.modmenu.ModMenuEntry"
    ]
  },
  "depends": {
    "minecraft": ${minecraft_dependency}
  },
  "custom": {
    "modmenu:clientsideOnly": true
  },
  "jars": ${sub_jars}
}
